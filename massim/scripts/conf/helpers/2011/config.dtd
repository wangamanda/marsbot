<!ATTLIST conf
	backuppath CDATA "backup"
	launch-sync-type CDATA "key"
	reportpath CDATA "./backup/"
	time-to-launch CDATA "10000"
	tournamentmode CDATA "2"
	tournamentname CDATA "Mars2011"
	>
	
<!ATTLIST simulation 
	configurationclass CDATA "massim.competition2011.GraphSimulationConfiguration"
	rmixmlobsserverhost CDATA "localhost"
	rmixmlobsserverport CDATA "1099"
	rmixmlobserver CDATA "massim.competition2011.GraphSimulationRMIXMLDocumentObserver"
	simulationclass CDATA "massim.competition2011.GraphSimulation"
	xmlstatisticsobserver CDATA "massim.competition2011.GraphSimulationXMLStatisticsObserver"
	visualisationobserver CDATA "massim.competition2011.GraphSimulationVisualizationObserver"
	visualisationobserver-outputpath CDATA "/home/massim/www/webapps/massim/output"
	rmixmlobserverweb CDATA "massim.competition2011.GraphSimulationRMIXMLDocumentObserverWebInterface"
	xmlobserver CDATA "massim.competition2011.GraphSimulationXMLObserver"
	xmlobserverpath CDATA "./backup/xmls"
	>
	
<!ATTLIST configuration
	xmlns:meta CDATA "http://www.tu-clausthal.de/"
	maxNumberOfSteps CDATA "750"
	numberOfAgents CDATA "20"
	numberOfTeams CDATA "2"
	agentsPerTeam CDATA "1"
	gridWidth CDATA "15"
	gridHeight CDATA "15"
	cellWidth CDATA "100"
	minNodeWeight CDATA "1"
	maxNodeWeight CDATA "10"
	minEdgeCost CDATA "1"
	maxEdgeCost CDATA "10"
	nodeWeighting CDATA "0"
	mapGenerator CDATA "GraphGeneratorTriangulationBalanced"
	>